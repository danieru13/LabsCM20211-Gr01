<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="icon_style">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">32dp</item>
    </style>

    <style name="textInput_style" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginEnd">32dp</item>
    </style>

    <style name="autocomple_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:completionThreshold">1</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:inputType">text</item>
        <item name="android:padding">@dimen/padding_autocomplete</item>
    </style>
</resources>